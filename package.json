    "mongodb": "^2.0.0",
    "mongoose": "^4.0.1",
    "react": "0.13.1",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "winston": "^0.9.0"
  },
  "devDependencies": {
    "browserify": "9.0.3",
    "glob": "^5.0.3",
    "gulp": "3.8.11",
    "gulp-browserify": "^0.5.1",
    "gulp-livereload": "^3.8.0",
    "gulp-minify-css": "^1.0.0",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "3.0.4",
    "nodemon": "1.3.7",
    "reactify": "1.1.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "3.0.0"
  },
  "engines": {
    "node": "0.10.x"
