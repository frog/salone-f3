{
  "name": "f3",
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ]
    ]
  },
  "version": "0.0.1",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "(gulp watch & echo $!> client.pid) && (node node_modules/nodemon/bin/nodemon app.js & echo $!> server.pid)",
    "stop": "kill -TERM $(cat client.pid); kill -TERM $(cat server.pid)"
  },
  "dependencies": {
